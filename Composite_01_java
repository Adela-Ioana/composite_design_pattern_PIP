package composite_design_pattern;

import java.util.ArrayList;
import java.util.List;

public class Composite_01 implements Component{
	
	
	   private List<Component> Lista1=new ArrayList<Component>();
	   
	   
	   public void print()
	   {
	        Lista1.forEach(Component::print);
	   }
	   
	   public void add(Component comp) 
	   {
	        Lista1.add(comp);
	   }
	   
	   public void remove(Component comp) 
	   {
	        Lista1.remove(comp);
	   }
}
