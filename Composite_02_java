package composite_design_pattern;

import java.util.ArrayList;
import java.util.List;

public class Composite_02 implements Component{
	
    private List<Component> Lista2 =new ArrayList<Component>();;
    
    
    public void print() 
    {
        Lista2.forEach(Component::print);
    }
 
    public void add(Component comp) 
    {
        Lista2.add(comp);
    }
 
    public void remove(Component comp) 
    {
        Lista2.remove(comp);
    }
}
